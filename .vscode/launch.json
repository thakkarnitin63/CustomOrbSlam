{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "justMyCode": true,
            "env": {
                "PYTHONPATH": "${workspaceFolder}"
            }
        },
        {
            "name": "Python: Run Module",
            "type": "python",
            "request": "launch",
            "module": "orbslam.examples.run_test",
            "console": "integratedTerminal",
            "justMyCode": true
        }
    ]
}
